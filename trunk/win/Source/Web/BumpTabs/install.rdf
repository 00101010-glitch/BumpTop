<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	 xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
	<em:id>bumptabs@bumptop.com</em:id>
	<em:version>$Rev$</em:version>
	<em:type>2</em:type>

	<!-- Target Application this extension can install into, 
		 with minimum and maximum supported versions. --> 
	<em:targetApplication>
	  <Description>
		<em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
		<em:minVersion>3.0</em:minVersion>
		<em:maxVersion>3.*</em:maxVersion>
	  </Description>
	</em:targetApplication>

	<!-- Front End MetaData -->
	<em:name>Bump these tabs</em:name>
	<em:description>Open the current set of browser tabs in BumpTop</em:description>
	<em:iconURL>chrome://bumptabs/skin/BumpTop32x32.png</em:iconURL>
	<em:creator>BumpTop</em:creator>
	<em:homepageURL>http://bumptop.com</em:homepageURL>
  </Description>      
</RDF>
